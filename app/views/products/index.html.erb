<h1>Products</h1>


<ul class="product-list">
<% @products.each do | product| %>
    <li>
        <%= link_to(
           product.title, product_path(product))%> <br /> 
            <small>
                Posted <%= time_ago_in_words(product.created_at)  %> ago
            </small>
                </li>
            <% end %>
</ul>