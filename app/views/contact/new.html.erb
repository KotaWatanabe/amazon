<h1>Contact Us</h1>

<%= form_tag contact_us_create_path do %>
    <div class="form">
    <div class="label-input">
    <%= label_tag :name %>
    <%= text_field_tag :name%>
  </div>
  <div class="label-input">
    <%= label_tag :email %>
    <%= email_field_tag :email%>
  </div>
  <div class="label-input">
    <%= label_tag :message %>
    <%= text_area_tag :message%>
  </div>
  <%= submit_tag 'Contact Us' %>
  </div>
 <% end %>